{
  "name": "mind-travel-api",
  "version": "0.3.0",
  "description": "Starter project for an ES6 RESTful Express API",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"ts-node-dev src\"",
    "build": "tsc",
    "start": "ts-node -T ./src/index.ts",
    "tslint": "tslint -c tslint.json 'src/**/*.ts'"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "license": "MIT",
  "dependencies": {
    "@uppy/companion": "^0.17.3",
    "aws-sdk": "^2.404.0",
    "babel-eslint": "^10.0.1",
    "bcrypt": "^3.0.4",
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "express": "^4.13.3",
    "express-jwt": "^5.3.1",
    "hash.js": "^1.1.7",
    "jsonwebtoken": "^8.5.0",
    "local-storage": "^1.4.2",
    "morgan": "^1.8.0",
    "resource-router-middleware": "^0.7.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-typescript": "^7.3.3",
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.4",
    "@types/express-jwt": "0.0.42",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/morgan": "^1.7.35",
    "@types/node": "^11.11.6",
    "@typescript-eslint/eslint-plugin": "^1.5.0",
    "@typescript-eslint/eslint-plugin-tslint": "^1.5.0",
    "@typescript-eslint/parser": "^1.5.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "nodemon": "^1.9.2",
    "ts-node": "^8.0.3",
    "ts-node-dev": "^1.0.0-pre.32",
    "tslint": "^5.14.0",
    "typescript": "^3.3.4000"
  }
}
